	<link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">


	<style type="text/css">
		.navbar-inverse{
			background: #2C3E50;


		}

		body{
			padding-top:120px;
		}

		.jumbotron{
			 color: #2C3E50;
		}

		.navbar-inverse .navbar-nav>li>a {
    		color: white;
		}

		.navbar .navbar-brand{
			color: white;
		}

	</style>

</head>

<body>
	<script src="http://code.jquery.com/jquery-3.2.1.min.js"
